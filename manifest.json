{
  "manifest_version": 3,
  "name": "Check SoulOverAI",
  "version": "1.0",
  "permissions": ["storage"],
  "web_accessible_resources": [{
    "resources": ["ArtistUsesAI.png","LooksHuman.png", "Hover.html"],
    "matches": ["<all_urls>"]
  }],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["https://music.youtube.com/*"],
    "js": ["Badge.js","YtMusic.js"]
  }]
}